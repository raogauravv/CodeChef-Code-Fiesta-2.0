   "source": [
    "n=int(input())\n",
    "l=[]\n",
    "sum=0\n",
    "for i in str(n):\n",
    "    l.append(i)\n",
    "for i in l:\n",
    "    sum+=(int(i)**len(l))\n",
    "if sum==n:\n",
    "    print(\"Yes\")"
